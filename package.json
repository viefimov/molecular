  "devDependencies": {
    "@chromatic-com/storybook": "3.2.3",
    "@eslint/js": "^9.17.0",
    "@moleculer/lab": "^0.6.5",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@storybook/addon-essentials": "8.6.14",
