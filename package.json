    "concurrently": "^9.1.2",
    "esbuild": "0.24.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^0.11.1",
    "glob": "^11.0.2",
    "globals": "^15.14.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-playwright-preset": "^4.0.0",
    "jest-runner": "^29.7.0",
    "moleculer-repl": "^0.7.4",
    "playwright": "^1.52.0",
    "playwright-chromium": "^1.52.0",
    "prettier": "^3.4.2",
    "sass": "^1.89.0",
    "storybook": "8.6.14",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.3.5",
    "wait-on": "^8.0.3"
  },
  "scripts": {
    "lint": "eslint ./src",
    "format": "prettier ./src --write",
    "api:build": "tsc --project tsconfig.node.json",
    "api:watch": "tsc --watch",
    "api:test": "jest",
    "api:dev": "cross-env NODE_ENV=development SERVICES=${0} tsx src/main.ts",
    "api:start": "cross-env NODE_ENV=production node dist/main.js",
    "ui:dev": "cross-env NODE_ENV=development VITE_APP=app-${0} vite",
    "ui:build": "cross-env VITE_APP=app-${0} vite build",
    "ui:watch": "cross-env VITE_APP=app-${0} vite build --watch",
    "ui:preview": "cross-env VITE_APP=app-${0} vite preview",
    "ui:test": "yarn test-storybook:ci",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn build-storybook --quiet && yarn http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook\""
  },
  "dependencies": {
    "bson": "^6.10.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "fastest-validator": "^1.19.0",
    "i18next": "^25.2.0",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "imask": "^7.6.1",
    "lodash": "^4.17.21",
    "moleculer": "^0.14.35",
    "moleculer-cli": "^0.8.1",
    "moleculer-web": "^0.10.8",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.1",
    "react-imask": "^7.6.1",
    "react-router": "^7.6.2"
  },
  "resolutions": {
    "esbuild": "0.24.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
